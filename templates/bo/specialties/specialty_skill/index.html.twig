{% extends 'base.html.twig' %}

{% block title %}SpecialtySkill index{% endblock %}

{% block body %}
    <h1>SpecialtySkill index</h1>
    <a href="{{ path('app_specialty_skill_new') }}">Create new</a>
    <br/><br/><br/>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for specialty_skill in specialty_skills %}
            <tr>
                <td>{{ specialty_skill.id }}</td>
                <td>{{ specialty_skill.name }}</td>
                <td>
                    <a href="{{ path('app_specialty_skill_edit', {'id': specialty_skill.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
